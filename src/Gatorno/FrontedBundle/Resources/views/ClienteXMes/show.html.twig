{% extends '::base.html.twig' %}

{% block body -%}
    <h1>ClienteXMes</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Clientexmes</th>
                <td>{{ entity.ClienteXMes }}</td>
            </tr>
            <tr>
                <th>Mes</th>
                <td>{{ entity.Mes|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fechasave</th>
                <td>{{ entity.FechaSave|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Porciento</th>
                <td>{{ entity.porciento }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('cliente_mes') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('cliente_mes_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
